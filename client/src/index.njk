	<div id="app"></div>
	<script type="module">
		const mod = import("./main.js");
		document.addEventListener("dblclick", (e) => {e.preventDefault();}, {passive: false});
		document.addEventListener('touchstart', (e) => {if (e.touches.length > 1) e.preventDefault();}, {
			passive: false
		});
		document.addEventListener('touchmove', (e) => {e.preventDefault();}, {passive: false});

		document.querySelector("#logo svg path").addEventListener("animationend", async () => {
			const {render} = await mod;
			render(document.getElementById("app"), () => {
				document.getElementById("logo").animate(
					[{opacity: 1}, {opacity: 0}],
					{duration: 500, easing: 'ease-in'}
				).finished.then(
					(a) => {
						a.effect.target.style.opacity = 0;
						a.effect.target.style.display = "none";
					}
				)
			})
		}, {once: true});
	</script>
</body>

</html>
